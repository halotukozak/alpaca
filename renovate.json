{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/(^|/)build\\.sc$/",
        "/(^|/)build\\.mill$/",
        "/\\.mill$/"
      ],
      "matchStrings": [
        "mvn\"(?<depName>[^:]+:[^:]+):(?<currentValue>[^\"]+)\""
      ],
      "datasourceTemplate": "maven"
    }
  ]
}
